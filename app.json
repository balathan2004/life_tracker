{
  "expo": {
    "name": "life_tracker",
    "slug": "life_tracker",
    "owner": "lightappdev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/life-tracker.png",
    "scheme": "lifetracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/life-tracker.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lightappdev.life_tracker",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/life-tracker.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "96f99753-0022-4d95-a4bf-ab8e735ccfd1"
      }
    }
  }
}
